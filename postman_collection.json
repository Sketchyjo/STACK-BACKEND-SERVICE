    {
      "name": "üîê Authentication",
      "description": "User authentication and account management endpoints",
      "item": [
        {
          "name": "Register User",
          "request": {
            "method": "POST",
            "header": [{"key": "Content-Type", "value": "application/json"}],
            "url": {
              "raw": "{{base_url}}/api/v1/auth/register",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "auth", "register"]
            },
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"{{email}}\",\n  \"password\": \"{{password}}\"\n}"
            },
            "description": "Register a new user account with email and password. Sends verification code to email."
          },
          "response": [
            {
              "name": "‚úÖ Registration Successful",
              "status": "Accepted",
              "code": 202,
              "header": [{"key": "Content-Type", "value": "application/json"}],
              "body": "{\n  \"message\": \"Verification code sent to alice@example.com. Please verify your account.\",\n  \"identifier\": \"alice@example.com\"\n}"
            }
